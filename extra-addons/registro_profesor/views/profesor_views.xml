<odoo>
    <record id="view_profesor_tree" model="ir.ui.view">
        <field name="name">profesor.tree</field>
        <field name="model">profesor</field>
        <field name="arch" type="xml">
            <tree string="Profesores">
                <field name="name"/>
                <field name="age"/>
                <field name="subject"/>
                <field name="email"/>
            </tree>
        </field>
    </record>

    <record id="view_profesor_form" model="ir.ui.view">
        <field name="name">profesor.form</field>
        <field name="model">profesor</field>
        <field name="arch" type="xml">
            <form string="Profesores">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="age"/>
                        <field name="subject"/>
                        <field name="email"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <menuitem id="menu_profesor_root" name="Profesores" sequence="10"/>
    <menuitem id="menu_profesor" name="Profesores" parent="menu_profesor_root" action="action_profesor"/>

    <menuitem id="menu_profesor_root" name="GestiÃ³n de Profesores" sequence="1"/>
    <menuitem id="menu_profesor" name="Profesores" parent="menu_profesor_root" action="registro_profesor.action_profesor"/>
 
    <record id="action_profesor" model="ir.actions.act_window">
        <field name="name">Profesores</field>
        <field name="res_model">profesor</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
